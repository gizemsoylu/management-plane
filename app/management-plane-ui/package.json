{
  "name": "management-plane-ui",
  "version": "0.0.1",
  "private": true,
  "description": "Management Plane Application",
  "keywords": [
    "ui5",
    "openui5",
    "sapui5"
  ],
  "main": "webapp/index.html",
  "devDependencies": {
    "@sap-ux/eslint-plugin-fiori-tools": "^0.4.0",
    "@sap/ui5-builder-webide-extension": "^1.1.9",
    "@sap/ux-specification": "^1.108.16",
    "@sap/ux-ui5-tooling": "1",
    "@sapui5/types": "~1.125.0",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@ui5/cli": "^3.0.0",
    "mbt": "^1.2.27",
    "typescript": "^5.1.6",
    "ui5-task-zipper": "^3.1.3",
    "ui5-tooling-modules": "^3.8.1",
    "ui5-tooling-transpile": "^3.3.7"
  },
  "scripts": {
    "build": "ui5 build --config=ui5.yaml --clean-dest --dest dist",
    "lint": "eslint ./",
    "ts-typecheck": "tsc --noEmit",
    "prestart": "npm run ts-typecheck",
    "prebuild": "npm run ts-typecheck",
    "deploy": "fiori verify",
    "deploy-config": "fiori add deploy-config",
    "build:cf": "ui5 build preload --clean-dest --config ui5-deploy.yaml --include-task=generateCachebusterInfo"
  },
  "ui5": {
    "dependencies": [
      "@sap/ui5-builder-webide-extension",
      "ui5-task-zipper",
      "mbt"
    ]
  },
  "dependencies": {
    "ui5-antares": "^1.124.999005"
  }
}
